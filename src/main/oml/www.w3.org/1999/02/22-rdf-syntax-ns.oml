@dc:^description="<p>This document defines an XML syntax for RDF called RDF/XML in terms of Namespaces in XML, the XML Information Set and XML Base.</p><p>This reflects just a subset of annotation properties and value types from RDF. Full documentation can be found at [https://www.w3.org/TR/rdf-syntax-grammar](https://www.w3.org/TR/rdf-syntax-grammar)</p>"
@dc:publisher="https://www.w3.org"
@dc:relation="https://www.w3.org/TR/rdf-syntax-grammar"
@dc:creator="World Wide Web Consortium"
open terminology <http://www.w3.org/1999/02/22-rdf-syntax-ns#> as rdf {

	extends <../../2001/XMLSchema.oml>

	@dc:^description="https://www.w3.org/TR/rdf-syntax-grammar/#aboutAttr"
	annotation property about

	@dc:^description="https://www.w3.org/TR/rdf-syntax-grammar/#section-literal-node"
	scalar PlainLiteral

	@dc:^description="https://www.w3.org/TR/rdf-syntax-grammar/#section-Syntax-XML-literals"
	scalar XMLLiteral
}
