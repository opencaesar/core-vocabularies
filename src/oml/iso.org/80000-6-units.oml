@dc:^source "IEC 80000-6:2008"
@dc:^source "https://www.iso.org/standard/31891.html"
vocabulary <http://iso.org/80000-6-units> with # as 80000-6-units {
 
    extends <http://iso.org/80000-5-units>

    aspect UnitOfElectricCurrentValue :> metrology:UnitaryQuantityValue [
        restricts all relation metrology:hasReference to UnitOfElectricCurrent
    ]
    @dc:^source "6-1"
    concept UnitOfElectricCurrent :> metrology:CoherentMeasurementUnit [
        restricts scalar property metrology:isDimensionlessMeasurementUnit to false^^xsd:boolean
    ]

    concept ampere :> UnitOfElectricCurrent, metrology:IndependentMeasurementUnit, 80000-concepts:MeasurementUnit 

    aspect UnitOfElectricChargeValue :> metrology:UnitaryQuantityValue [
        restricts all relation metrology:hasReference to UnitOfElectricCharge
    ]
    @dc:^source "6-2"
    concept UnitOfElectricCharge :> metrology:CoherentMeasurementUnit [
        restricts scalar property metrology:isDimensionlessMeasurementUnit to false^^xsd:boolean
    ]

    concept coulomb :> UnitOfElectricCharge, metrology:DerivedMeasurementUnit, 80000-concepts:MeasurementUnit 

    relation entity coulomb-of-ampere :> metrology:HasFactorForMeasurementUnit [
        from coulomb
        to ampere
        forward coulomb-of-ampere-factor
        restricts scalar property metrology:exponent to "1/1"^^owl:rational
    ]

    relation entity coulomb-of-second :> metrology:HasFactorForMeasurementUnit [
        from coulomb
        to 80000-3-units:second
        forward coulomb-of-second-factor
        restricts scalar property metrology:exponent to "1/1"^^owl:rational
    ]

    aspect UnitOfElectricFieldStrengthValue :> metrology:UnitaryQuantityValue [
        restricts all relation metrology:hasReference to UnitOfElectricFieldStrength
    ]
    @dc:^source "6-10"
    concept UnitOfElectricFieldStrength :> metrology:CoherentMeasurementUnit [
        restricts scalar property metrology:isDimensionlessMeasurementUnit to false^^xsd:boolean
    ]

    concept newton-per-coulomb :> UnitOfElectricFieldStrength, metrology:DerivedMeasurementUnit, 80000-concepts:MeasurementUnit 

    relation entity newton-per-coulomb-of-newton :> metrology:HasFactorForMeasurementUnit [
        from newton-per-coulomb
        to 80000-4-units:newton
        forward newton-per-coulomb-of-newton-factor
        restricts scalar property metrology:exponent to "1/1"^^owl:rational
    ]

    relation entity newton-per-coulomb-of-coulomb :> metrology:HasFactorForMeasurementUnit [
        from newton-per-coulomb
        to coulomb
        forward newton-per-coulomb-of-coulomb-factor
        restricts scalar property metrology:exponent to "-1/1"^^owl:rational
    ]

    aspect UnitOfElectricPotentialValue :> metrology:UnitaryQuantityValue [
        restricts all relation metrology:hasReference to UnitOfElectricPotential
    ]
    
    @dc:^source "6-11"
    concept UnitOfElectricPotential :> metrology:CoherentMeasurementUnit [
        restricts scalar property metrology:isDimensionlessMeasurementUnit to false^^xsd:boolean
    ]

    concept volt :> UnitOfElectricPotential, metrology:DerivedMeasurementUnit, 80000-concepts:MeasurementUnit 

    relation entity volt-of-newton-per-coulomb :> metrology:HasFactorForMeasurementUnit [
        from volt
        to newton-per-coulomb
        forward volt-of-newton-per-coulomb-factor
        restricts scalar property metrology:exponent to "1/1"^^owl:rational
    ]

    relation entity volt-of-metre :> metrology:HasFactorForMeasurementUnit [
        from volt
        to 80000-3-units:metre
        forward volt-of-metre-factor
        restricts scalar property metrology:exponent to "1/1"^^owl:rational
    ]

    aspect UnitOfResistanceValue :> metrology:UnitaryQuantityValue [
        restricts all relation metrology:hasReference to UnitOfResistance
    ]
    @dc:^source "6-46"
    concept UnitOfResistance :> metrology:CoherentMeasurementUnit [
        restricts scalar property metrology:isDimensionlessMeasurementUnit to false^^xsd:boolean
    ]

    
    concept ohm :> UnitOfResistance, metrology:DerivedMeasurementUnit, 80000-concepts:MeasurementUnit 

    relation entity ohm-of-volt :> metrology:HasFactorForMeasurementUnit [
        from ohm
        to volt
        forward ohm-of-volt-factor
        restricts scalar property metrology:exponent to "1/1"^^owl:rational
    ]

    relation entity ohm-of-ampere :> metrology:HasFactorForMeasurementUnit [
        from ohm
        to ampere
        forward ohm-of-ampere-factor
        restricts scalar property metrology:exponent to "-1/1"^^owl:rational
    ]
}

